{
  "disable_mob_generation": false,
  "ore_veins_enabled": false,
  "aquifers_enabled": false,
  "legacy_random_source": false,
  "sea_level": 0,

  "default_block": { "Name": "minecraft:purpur_block" },
  "default_fluid": { "Name": "minecraft:air" },

  "noise": {
    "min_y": -64,
    "height": 384,
    "size_horizontal": 1,
    "size_vertical": 2
  },

    "noise_router": {
      "barrier": 0,
      "continents": 0,
      "erosion": 0,
      "depth": 0,
      "ridges": 0,

      "temperature": 0,
      "vegetation": 0,

    "vein_toggle": 0,
    "vein_ridged": 0,
    "vein_gap": 0,

    "initial_density_without_jaggedness": 0,
    "fluid_level_floodedness": 0,
    "fluid_level_spread": 0,
    "lava": 0,

    "final_density": {
      "type": "minecraft:mul",
      "argument1": 1.5,
      "argument2": { "type": "minecraft:end_islands" }
    }
  },

  "spawn_target": [],

  "surface_rule": {
    "type": "minecraft:sequence",
    "sequence": [
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:biome",
          "biome_is": ["dd:fairy_isles"]
        },
        "then_run": {
          "type": "minecraft:sequence",
          "sequence": [
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:stone_depth",
                "offset": 0,
                "add_surface_depth": true,
                "surface_type": "floor",
                "secondary_depth_range": 0
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": { "Name": "minecraft:purple_terracotta" }
              }
            },
            {
              "type": "minecraft:condition",
              "if_true": {
                "type": "minecraft:stone_depth",
                "offset": 1,
                "add_surface_depth": true,
                "surface_type": "floor",
                "secondary_depth_range": 0
              },
              "then_run": {
                "type": "minecraft:block",
                "result_state": { "Name": "minecraft:cyan_terracotta" }
              }
            }
          ]
        }
      },
      { "type": "minecraft:block", "result_state": { "Name": "minecraft:purpur_block" } }
    ]
  }
}
