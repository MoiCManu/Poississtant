{
  "disable_mob_generation": false,
  "ore_veins_enabled": false,
  "aquifers_enabled": false,
  "legacy_random_source": false,
  "sea_level": 0,

  "default_block": { "Name": "minecraft:purpur_block" },
  "default_fluid": { "Name": "minecraft:air" },

  "noise": {
    "min_y": -64,
    "height": 384,
    "size_horizontal": 1,
    "size_vertical": 2
  },

  "noise_router": {
    "barrier": 0,
    "continents": "minecraft:overworld/continents",
    "erosion":    "minecraft:overworld/erosion",
    "depth":      "minecraft:overworld/depth",
    "ridges":     "minecraft:overworld/ridges",

    "temperature": {
      "type": "minecraft:shifted_noise",
      "noise": "minecraft:temperature",
      "shift_x": "minecraft:shift_x",
      "shift_y": 0,
      "shift_z": "minecraft:shift_z",
      "xz_scale": 0.25,
      "y_scale": 0
    },
    "humidity": "minecraft:overworld/humidity",
    "vegetation": {
      "type": "minecraft:shifted_noise",
      "noise": "minecraft:vegetation",
      "shift_x": "minecraft:shift_x",
      "shift_y": 0,
      "shift_z": "minecraft:shift_z",
      "xz_scale": 0.25,
      "y_scale": 0
    },

    "initial_density_without_jaggedness": "minecraft:overworld/initial_density_without_jaggedness",
    "fluid_level_floodedness": 0,
    "fluid_level_spread": 0,
    "lava": -1,

    "vein_toggle": 0,
    "vein_ridged": 0,
    "vein_gap": 0,

    "final_density": {
      "type": "minecraft:mul",
      "argument1": 1.5,
      "argument2": { "type": "minecraft:end_islands" }
    }
  },

  "spawn_target": [],

  "surface_rule": {
    "type": "minecraft:sequence",
    "sequence": [
      {
        "type": "minecraft:condition",
        "if_true": { "type": "minecraft:biome", "biome_is": [ "dd:fairy_isles" ] },
        "then_run": {
          "type": "minecraft:sequence",
          "sequence": [
            {
              "type": "minecraft:condition",
              "if_true": { "type": "minecraft:stone_depth", "offset": 0, "add_surface_depth": true, "surface_type": "floor", "secondary_depth_range": 0 },
              "then_run": { "type": "minecraft:block", "result_state": { "Name": "minecraft:purple_terracotta" } }
            },
            {
              "type": "minecraft:condition",
              "if_true": { "type": "minecraft:stone_depth", "offset": 1, "add_surface_depth": true, "surface_type": "floor", "secondary_depth_range": 0 },
              "then_run": { "type": "minecraft:block", "result_state": { "Name": "minecraft:cyan_terracotta" } }
            }
          ]
        }
      },
      { "type": "minecraft:block", "result_state": { "Name": "minecraft:purpur_block" } }
    ]
  }
}
